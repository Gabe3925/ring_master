<%= form_for @band do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :genre %>
  <%= f.text_field :genre %>

  <%= f.label :video_url %>
  <%= f.text_field :video_url %>

  <%= f.label :kid_friendly %>
  <%= f.check_box :kid_friendly %>

  <%= f.submit "Submit" %>
<% end %>
